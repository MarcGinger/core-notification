{
  "name": "gsnest-microservice-starter",
  "version": "1.0.0",
  "description": "A parameter-driven, code-generation-ready NestJS microservice starter for DDD, CQRS, and event-sourced architectures. Provides a vibe code-ready platform for value objects, domain services, and aggregates.",
  "author": "Marc Ginger <marc.ginger@example.com>",
  "private": true,
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docs": "npx compodoc -p tsconfig.json",
    "watch-app": "nodemon --watch tools/domain/schema.dmm --watch tools/domain/parameters.json --exec \"node tools/domain/cqrs.js\""
  },
  "dependencies": {
    "@azure/storage-blob": "^12.28.0",
    "@eventstore/db-client": "^6.2.1",
    "@liaoliaots/nestjs-redis": "^10.0.0",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/express-handlebars": "^5.3.1",
    "@types/multer": "^2.0.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "bullmq": "^5.56.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express-handlebars": "^8.0.2",
    "ioredis": "^5.6.1",
    "keycloak-connect": "^26.1.1",
    "lodash": "^4.17.21",
    "nest-keycloak-connect": "^2.0.0-alpha.0",
    "nestjs-pino": "^4.4.0",
    "pg": "^8.16.0",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "pino-roll": "^3.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@compodoc/compodoc": "^1.1.26",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.29",
    "@types/ajv": "^0.0.5",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "overrides": {
    "@liaoliaots/nestjs-redis-health": {
      "@nestjs/common": "^11.1.0",
      "@nestjs/core": "^11.1.0",
      "@nestjs/terminus": "^11.0.0"
    },
    "@liaoliaots/nestjs-redis": {
      "@nestjs/common": "^11.1.0",
      "@nestjs/core": "^11.1.0"
    }
  }
}
